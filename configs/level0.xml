<?xml version="1.0" encoding="UTF-8"?>

<seqgra xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<general id="g1">
		<label>grammar level 0</label>
		<description>two conditions, one fixed sequence element specific to each</description>
		<sequencespace>DNA</sequencespace>
	</general>

	<background>
		<minlength>150</minlength>
		<maxlength>150</maxlength>
		<alphabetdistributions>
			<alphabetdistribution>
				<letter probability="0.25">A</letter>
				<letter probability="0.25">C</letter>
				<letter probability="0.25">G</letter>
				<letter probability="0.25">T</letter>
			</alphabetdistribution>
		</alphabetdistributions>
	</background>

	<datageneration>
		<seed>0</seed>
		<sets>
			<set name="training">
				<conditionrefs>
					<conditionref cid="c1" samples="10000" />
					<conditionref cid="c2" samples="10000" />
				</conditionrefs>
			</set>
			<set name="validation">
				<conditionrefs>
					<conditionref cid="c1" samples="1000" />
					<conditionref cid="c2" samples="1000" />
				</conditionrefs>
			</set>
			<set name="test">
				<conditionrefs>
					<conditionref cid="c1" samples="5000" />
					<conditionref cid="c2" samples="5000" />
				</conditionrefs>
			</set>
		</sets>
	</datageneration>

	<conditions>
		<condition id="c1">
			<label>cell type 1</label>
			<description></description>
			<grammar>
				<rule>
					<position>90</position>
					<probability>1.0</probability>
					<sequenceelementrefs>
						<sequenceelementref sid="se1" />
					</sequenceelementrefs>
				</rule>
			</grammar>
		</condition>
		<condition id="c2">
			<label>cell type 2</label>
			<description></description>
			<grammar>
				<rule>
					<position>70</position>
					<probability>1.0</probability>
					<sequenceelementrefs>
						<sequenceelementref sid="se2" />
					</sequenceelementrefs>
				</rule>
			</grammar>
		</condition>
	</conditions>

	<sequenceelements>
		<sequenceelement id="se1">
			<kmerbased>
				<kmer probability="1.0">AAAAA</kmer>
			</kmerbased>
		</sequenceelement>
		<sequenceelement id="se2">
			<kmerbased>
				<kmer probability="1.0">GGGGG</kmer>
			</kmerbased>
		</sequenceelement>
	</sequenceelements>
</seqgra>
